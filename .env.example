# Context7 MCP Configuration
CONTEXT7_API_KEY=your_context7_api_key_here

# GitHub Integration
GITHUB_TOKEN=your_github_personal_access_token_here

# PostgreSQL Connection
POSTGRES_CONNECTION_STRING=postgresql://username:password@localhost:5432/elfi_db

# Zenoh Network Configuration (optional overrides)
ZENOH_ENDPOINTS=tcp/localhost:7447
ZENOH_MODE=peer

# ELFI Storage Configuration
ELFI_STORAGE_PATH=./elfi-storage
ELFI_CACHE_SIZE=1000000

# Security Settings
JWT_SECRET=your_jwt_secret_here
TLS_CERT_PATH=./certs/server.crt
TLS_KEY_PATH=./certs/server.key